server {
    listen 8080;
    server_name genai.amprajin.in;
    root /app;
 
    location /status {
        stub_status on;
        access_log   off;
        allow all;
    }
    location ~ /.well-known {
      allow all;
    }
    
    location / {
        proxy_pass http://localhost:8080;
    }  
}
